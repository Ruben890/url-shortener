{
  "builds": [
    {
      "src": "config/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "config/wsgi.py"
    }
  ],
  "functions": {
    "migrate": {
      "handler": "vercel/python",
      "includeFiles": "config/",
      "runtime": "vercel/python",
      "memory": 1024
    }
  }
}
